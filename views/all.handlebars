<div class="menu-container">
  <div class="card-header col-md-10">
    <h2>

    </h2>
  </div>
  <!-- For each dish in the array of dish objects, the following <div> will be rendered. -->
  {{#each dishes as |dish|}}
    <div class="dish col-md-5">
      <!-- We use dish.id instead of <od> and <li> elements. -->
      <p>
        {{blog.blog_title}}.
        {{blog.author_name}}
      </p>
    </div>
    <div class="col-md-7">
      <p>
        Blog Body
        {{blog.blog_body}}
      </p>
    </div>
  {{/each}}
</div>